.hamburger {
  padding: 0px;
  display: none;
}
.hamburger:active {
  outline: none;
}
.hamburger:focus {
  outline: none;
}
.hamburger-box {
  width: 30px;
}
.hamburger-inner {
  width: 30px;
  height: 3px;
  background-color: #ff9a00;
}
.hamburger-inner::after {
  top: -14px !important;
  width: 30px;
  height: 3px;
  background-color: #ff9a00;
}
.hamburger-inner::before {
  top: -7px;
  width: 30px;
  height: 3px;
  background-color: #ff9a00;
}
